{
  "name": "meli-challenge-api",
  "version": "0.1.0",
  "description": "Meli challenge api",
  "engines": {
    "node": "10.14.1",
    "npm": "6.4.1"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit && eslint \"*/**/*.ts\"",
    "lint-diff": "git diff --name-only --cached --relative | grep \\\\.ts$ | xargs eslint",
    "lint-fix": "eslint \"**/*.ts\" --fix",
    "migrations": "ts-node-dev ./node_modules/typeorm/cli.js migration:run --config ./config/typeorm.ts",
    "migrations-undo": "ts-node-dev ./node_modules/typeorm/cli.js migration:revert --config ./config/typeorm.ts",
    "migrations-test": "NODE_ENV=testing ts-node-dev ./node_modules/typeorm/cli.js migration:run --config ./config/typeorm.ts",
    "migrations-create": "ts-node-dev ./node_modules/typeorm/cli.js migration:create --config ./config/typeorm.ts -n",
    "migrations-generate": "ts-node-dev ./node_modules/typeorm/cli.js migration:generate --config ./config/typeorm.ts -n",
    "outdated": "npm outdated --depth 0",
    "postinstall": "npm run build",
    "prestart": "npm run lint",
    "start": "ts-node-dev --respawn --transpileOnly ./server.ts",
    "prod": "npm run build && node ./dist/server.js"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "main": "app.js",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "http-status-codes": "^1.4.0",
    "jwt-simple": "^0.5.1",
    "pg": "^7.18.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.22",
    "umzug": "^2.1.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/babel-core": "^6.25.6",
    "@types/babel-traverse": "^6.25.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/jwt-simple": "^0.5.33",
    "@types/node": "^13.7.6",
    "@types/pg": "^7.14.1",
    "@types/request-promise": "^4.1.44",
    "@types/umzug": "^2.2.3",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel": "6.23.0",
    "babel-core": "6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "dictum.js": "^1.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-wolox": "^4.0.0",
    "eslint-config-wolox-node": "^3.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "nock": "^12.0.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.0",
    "prompt": "^1.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-node-dev": "1.0.0-pre.44",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
